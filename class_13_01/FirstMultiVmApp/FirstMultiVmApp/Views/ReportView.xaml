<UserControl x:Class="FirstMultiVmApp.Views.ReportView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FirstMultiVmApp.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" 
             DataContext="{Binding Reports, Mode=OneWay, Source={StaticResource Provider}}">
    <Grid Background="LightBlue">
        <Label Content="Reporting" VerticalAlignment="Center" HorizontalAlignment="Center"/>
    </Grid>
</UserControl>
